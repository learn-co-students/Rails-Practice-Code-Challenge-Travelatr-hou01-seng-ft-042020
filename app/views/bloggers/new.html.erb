<% if flash[:errors]%>
    <% flash[:errors].each do |e| %>
        <li style="color: red">
            <%= e %>
        </li>
    <%end%>
<%end%>
<h3>New Form</h3>
<%= form_with model: @blogger do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name%>
    <br>
    <%= f.label :bio%>
    <%= f.text_field :bio%>
    <br>
    <%= f.label :age %>
    <%= f.number_field :age%>
    <%= f.submit%>
<%end%>